{
	"QMainWindow":[
		{
			"tittle":"Ground Control Station",
			"icon":":/icons/feather/sun.svg",
			"frameless": true,
			"transluscentBg": true,
			"sizeGrip": "size_grip",
			"navigation":[
				{
					"minimize":"minimizeButton",
					"close": "exitButton",
					"restore":[
						{
							"buttonName": "restoreButton",
							"normalIcon": ":/icons/feather/maximize-2.svg",
							"maximizedIcon": ":/icons/feather/minimize-2.svg"
						}
					],
					"moveWindow": "frame_7",
					"tittleBar": "frame_7"
				}
			]
		}
	],
	"QCustomSlideMenu": [
		{
		"name": "leftMenuSubContainer",
		"defaultSize": [{
			"width": 40,
			"height": "parent"
		}],
		"collapsedSize": [{
			"width": 40,
			"height": "parent"
		}],
		"expandedSize": [{
			"width": 105,
			"height": "parent"
		}],
		"toggleButton": [{
			"buttonName": "menuButton",
			"icons": [{
				"whenMenuIsCollapsed": ":/icons/feather/align-justify.svg",
				"whenMenuIsExpanded": ":/icons/feather/chevron-left.svg"
			}]
		}],
		"menuTransitionAnimation": [{
			"animationDuration": 200,
			"animationEasingCurve": "Linear"
		}]
	},
		{
		"name": "centralMenuContainer",
		"defaultSize": [{
			"width": 0,
			"height": "parent"
		}],
		"collapsedSize": [{
			"width": 0,
			"height": "parent"
		}],
		"expandedSize": [{
			"width": 200,
			"height": "parent"
		}],
		"menuTransitionAnimation": [{
			"animationDuration": 200,
			"animationEasingCurve": "Linear"
		}]
	}
	],
	"QStackedWidget":[
		{
			"name":"mainPages",
			"navigation":[
				{
					"navigationButtons":[
						{
							"ConnectionButton": "ConnectionPage",
							"SettingsButton": "SettingsPage",
							"HomeButton": "HomePage",
							"ControlsButton": "ControlsPage"
						}
					]
				}
			]

		}
	],
	"QPushButtonGroup": [{
		"Buttons": [
			"ConnectionButton",
			"ControlsButton",
			"HomeButton",
			"moveButton",
			"menuButton",
			"AlarmsButton",
			"SettingsButton"
		],
		"Style":[{
			"Active": "background-color: #B4C5E4;",
			"NotActive": ""
		}]
	},
	{
		"Buttons": [
			"armBtn",
			"disarmBtn"
		],
		"theme": 2,
		"Style":[{
			"Active": "background-color: #fff; border-style: solid; border-radius: 2 px;",
			"NotActive": "border-style: solid; border-radius: 2 px;"
		}]
	}]
}